{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/save-news.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/save-news",
      "dest": "/api/save-news.js"
    },
    {
      "src": "/bundle.js",
      "dest": "/bundle.js"
    },
    {
      "src": "/bundle.js.LICENSE.txt",
      "dest": "/bundle.js.LICENSE.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
